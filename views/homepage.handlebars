<ol class="post-list">
    {{#each posts as |post|}}
    <li>
        <article class="post">
            <div>
                <h1 class="post-title">{{post.title}}</h1>
                <p class="post-content">{{post.content}}</p>                
            </div>
        </article>
    </li>
    {{/each}}
</ol>